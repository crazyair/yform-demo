{
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "htmlWhitespaceSensitivity": "css",
    "proseWrap": "always",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": { "parser": "json" }
        }
    ]
}
